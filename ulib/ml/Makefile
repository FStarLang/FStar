.PHONY: intfiles

all: intfiles
	+$(MAKE) -C .. rebuild

intfiles:			\
	FStar_Int8.ml		\
	FStar_Int16.ml		\
	FStar_Int32.ml		\
	FStar_Int64.ml		\
	FStar_UInt16.ml		\
	FStar_UInt32.ml		\
	FStar_UInt64.ml

%.ml: %.ml.prefix FStar_Ints.ml.body
	@>$@
	@echo '(* !!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!! *)' >> $@
	@echo '(* THIS IS AN AUTOGENERATED FILE! See ulib/ml/Makefile *)' >> $@
	@echo '(* !!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!! *)' >> $@
	@echo '' >> $@
	@cat $< FStar_Ints.ml.body >> $@

clean:
	+$(MAKE) -C .. clean_ocaml
